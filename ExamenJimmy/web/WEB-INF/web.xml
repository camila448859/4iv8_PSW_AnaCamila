<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>registroalumno</servlet-name>
        <servlet-class>registroalumno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consulta</servlet-name>
        <servlet-class>consulta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consulta4</servlet-name>
        <servlet-class>consulta4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultabases</servlet-name>
        <servlet-class>consultabases</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>desarrollo</servlet-name>
        <servlet-class>desarrollo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>nuevastecnologias</servlet-name>
        <servlet-class>nuevastecnologias</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registroalumno</servlet-name>
        <url-pattern>/registroalumno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consulta</servlet-name>
        <url-pattern>/consulta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consulta4</servlet-name>
        <url-pattern>/consulta4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultabases</servlet-name>
        <url-pattern>/consultabases</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>desarrollo</servlet-name>
        <url-pattern>/desarrollo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nuevastecnologias</servlet-name>
        <url-pattern>/nuevastecnologias</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
